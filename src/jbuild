(library
 ((name livereload)
  (public_name livereload)
  (preprocess (pps (ppx_blob)))
  (preprocessor_deps ((file ../static/livereload.js)))
  (libraries (containers websocket-lwt.cohttp lwt
              (select backend.ml from
